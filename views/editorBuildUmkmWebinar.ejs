<%- include("./templates/header") %> 
<div class="editorBuildUmkmWebinar">
    <div class="sign-editorBuildUmkmWebinar d-flex align-items-center justify-content-center mb-4">
        <h1 class="m-0">Editor Webinar</h1>
    </div>
    <form class="d-flex flex-column align-items-center" action="/input/buildUmkm/webinar" method="post" enctype="multipart/form-data">
        <div id="webinarBuildUmkmPamf" class="webinarBuildUmkmPamf mb-4"></div>
        <input type="file" accept="image/*" name="pamfletBuildUmkm" id="pamfletBuildUmkm">

        <input class="d-block mt-2 mb-4 px-4 py-3 rounded-2" type="text" name="webinarBuildUmkmTitle" id="webinarBuildUmkmTitle" placeholder="Masukan Judul ..." required>
        <textarea class="d-block  rounded-2 p-4 mb-4" name="webinarBuildUmkmDescription" id="webinarBuildUmkmDescription" placeholder="Deskripsi Modul ..." required></textarea>
        <input class="d-block mt-2 mb-4 px-4 py-3 rounded-2" type="datetime-local" name="dateBuildUmkm" id="dateBuildUmkm">
        <input class="d-block mt-2 mb-4 px-4 py-3 rounded-2" type="text" name="webinarBuildUmkmLink" id="webinarBuildUmkmLink" placeholder="Masukan Link Webinar ..." required>
        
        <button type="submit" class="btn btn-success">Submit</button>
    </form>
</div>
<%- include("./templates/footer") %> 